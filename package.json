{
  "name": "tods-tmx-classic-converter",
  "version": "2.13.7",
  "keywords": [
    "TODS",
    "tournament",
    "tournaments",
    "competition",
    "tennis"
  ],
  "description": "converts legacy tmx tournament records to TODS",
  "repository": {
    "type": "git",
    "url": "https://github.com/CourtHive/tods-tmx-classic-converter"
  },
  "homepage": "https://github.com/CourtHive/tods-tmx-classic-converter",
  "bugs": {
    "url": "https://github.com/CourtHive/tods-tmx-classic-converter",
    "email": "support@CourtHive.com"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "np": {
    "contents": "."
  },
  "engines": {
    "node": ">=16.16.0 < 19"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "release": "np --no-tests",
    "prepare": "tsdx build",
    "start": "tsdx watch",
    "build": "tsdx build"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Charles Allen <charles@CourtHive.com> (CourtHive.com)",
  "license": "MIT",
  "dependencies": {
    "circular-json": "^0.5.9",
    "cli-progress": "^3.9.0",
    "d3": "5.16.0",
    "date-fns": "^3.0.0",
    "tods-competition-factory": "^1.8.26"
  },
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/preset-env": "7.23.3",
    "esm": "^3.2.25",
    "np": "^8.0.2",
    "tsdx": "^0.14.1"
  }
}
