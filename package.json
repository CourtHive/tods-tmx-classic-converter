{
  "name": "tods-tmx-classic-converter",
  "version": "1.0.14",
  "keywords": [
    "TODS",
    "tournament",
    "tournaments",
    "competition",
    "tennis"
  ],
  "description": "converts legacy tmx tournament records to TODS",
  "repository": {
    "type": "git",
    "url": "https://github.com/CourtHive/tods-tmx-classic-converter"
  },
  "homepage": "https://github.com/CourtHive/tods-tmx-classic-converter",
  "bugs": {
    "url": "https://github.com/CourtHive/tods-tmx-classic-converter",
    "email": "support@CourtHive.com"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "np": {
    "contents": "."
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "release": "np --no-tests",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "tsdx build",
